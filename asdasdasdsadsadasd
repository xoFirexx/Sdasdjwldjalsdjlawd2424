            ctx.lineJoin = "round";
            ctx.moveTo(27 * scale, 14.399993896484375 * scale);
            ctx.bezierCurveTo(29 * scale, 4.399993896484375 * scale, 44 * scale, 16.399993896484375 * scale, 37 * scale, 21.399993896484375 * scale);
