        time: ((1000 * 60) * 8) * 6
    }, {
        time: ((1000 * 60) * 8) * 5
    }, {
        time: ((1000 * 60) * 8) * 5
    }, {
        time: ((1000 * 60) * 8) * 10
    }, {
        time: ((1000 * 60) * 8) * 6
    }, {
        time: ((1000 * 60) * 8) * 7
    }, {
        time: ((1000 * 60) * 8) * 6
    }, {
        time: ((1000 * 60) * 8) * 8
    }, {
        time: ((1000 * 60) * 8) * 7
    }];
    var ITEMS = {
        PLAYERS: 0,
        FIRE: 1,
        WORKBENCH: 2,
        SEED: 3,
        WALL: 4,
        SPIKE: 5,
        BIG_FIRE: 6,
        STONE_WALL: 7,
        GOLD_WALL: 8,
        DIAMOND_WALL: 9,
        WOOD_DOOR: 10,
        CHEST: 11,
        STONE_SPIKE: 12,
        GOLD_SPIKE: 13,
        DIAMOND_SPIKE: 14,
        STONE_DOOR: 15,
        GOLD_DOOR: 16,
        DIAMOND_DOOR: 17,
        FURNACE: 18,
        AMETHYST_WALL: 19,
