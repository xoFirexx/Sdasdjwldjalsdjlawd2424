                if (this.spin_win_effect > 6) {
                    this.spin = 0;
                    select_subview(__SHOP__);
                }
                ctx.globalAlpha = 1;
            }
            user.alert.draw("#FFF", "#000");
            if (_this.waiting)
                this.loading.draw();

        };
        var COUNTER = 0;
        var __GAME__ = Math.pow(2, COUNTER++);
