        ctx.save();
        ctx.translate(298.5 * scale, 210 * scale);
        round_rect(ctx, -20.5 * scale, -109 * scale, 41 * scale, 218 * scale, 10 * scale);
        ctx.restore();
        fill_path(ctx, color[3], color[1], 12 * scale);
        ctx.save();
        ctx.translate(108.5 * scale, 210 * scale);
