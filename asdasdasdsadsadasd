            ctx.translate(-32 * scale, -19 * scale);
            ctx.beginPath();
            ctx.lineCap = "round";
            ctx.lineJoin = "round";
            ctx.moveTo(123 * scale, 46 * scale);
            ctx.bezierCurveTo(132 * scale, 46 * scale, 135 * scale, 39 * scale, 134 * scale, 33 * scale);
