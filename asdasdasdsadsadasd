            ctx.globalAlpha = 1;
            ctx.translate(-3 * scale, -3 * scale);
            ctx.beginPath();
            ctx.lineCap = "round";
            ctx.lineJoin = "round";
            ctx.moveTo(176 * scale, 10.25 * scale);
