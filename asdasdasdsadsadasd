                var p = players[l[i]["i"]];
                p.nickname = l[i]["n"];
                p.skin = l[i]["s"];
                p.accessory = l[i]["a"];
                p.crate = l[i]["c"];
                p.book = l[i]["b"];
                p.dead = l[i]["d"];
                p.baglook = l[i]["g"];
                p.level = l[i]["l"];
                p.score = Utils.restore_number(l[i]["p"]);
                p.alive = true;
                if (SHOW_ID === 1)
                    p.nickname += "|" + l[i]["i"];

            }
            user.ldb.sort();
