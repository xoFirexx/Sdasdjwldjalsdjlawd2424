        ctx.bezierCurveTo(97.75 * scale, 64.953125 * scale, 97.75 * scale, 64.953125 * scale, 92.25 * scale, 65.953125 * scale);
        ctx.closePath();
        fill_path(ctx, color[0]);
        ctx.globalAlpha = 1;
        ctx.translate(-5 * scale, -5 * scale);
        ctx.beginPath();
        ctx.lineCap = "round";
        ctx.lineJoin = "round";
