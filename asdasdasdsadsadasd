        ctx.bezierCurveTo(75 * scale, 224 * scale, 86 * scale, 228 * scale, 93 * scale, 228 * scale);
        ctx.bezierCurveTo(104 * scale, 229 * scale, 116 * scale, 219 * scale, 122 * scale, 205 * scale);
