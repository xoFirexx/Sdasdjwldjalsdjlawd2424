    function create_diamond_helmet(scale, draw_shadow, color) {
        var can = document.createElement("canvas");
        var ctx = can.getContext("2d");
        can.width = 142 * scale;
        can.height = 118 * scale;
        ctx.beginPath();
        ctx.lineCap = "round";
        ctx.lineJoin = "round";
