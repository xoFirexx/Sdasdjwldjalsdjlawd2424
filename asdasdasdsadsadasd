            ctx.lineJoin = "round";
            ctx.moveTo(99 * scale, 101 * scale);
            ctx.bezierCurveTo(118.6666259765625 * scale, 104.33331298828125 * scale, 124.6666259765625 * scale, 116.33331298828125 * scale, 112.6666259765625 * scale, 114.33331298828125 * scale);
