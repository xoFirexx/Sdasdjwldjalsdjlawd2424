        ctx.lineJoin = "round";
        ctx.moveTo(115 * scale, 211.20000457763672 * scale);
        ctx.bezierCurveTo(106 * scale, 216.20000457763672 * scale, 101 * scale, 234.20000457763672 * scale, 102 * scale, 241.20000457763672 * scale);
