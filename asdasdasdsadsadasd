        ctx.bezierCurveTo(108 * scale, 276 * scale, 112 * scale, 222 * scale, 213 * scale, 208 * scale);
        ctx.closePath();
        fill_path(ctx, color[3], color[4], 14 * scale);
        ctx.beginPath();
