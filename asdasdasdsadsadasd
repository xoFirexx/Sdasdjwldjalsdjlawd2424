            ctxDrawImage(ctx, img, (-w * scale) / 4, (-h * scale) / 4, w / 2, h / 2);
            ctx.globalAlpha = 1;
        }
        ctx.restore();
        ctx.restore();
    };

    function draw_baby_dragon() {
        ctx.save();
