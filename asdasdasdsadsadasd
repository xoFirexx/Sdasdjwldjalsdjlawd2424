        ctx.bezierCurveTo(44 * scale, 202 * scale, 49 * scale, 191 * scale, 52 * scale, 172 * scale);
        ctx.closePath();
        fill_path(ctx, color[7], color[8], 6 * scale);
        ctx.beginPath();
