        ctx.bezierCurveTo(267 * scale, 236 * scale, 292 * scale, 263 * scale, 377 * scale, 284 * scale);
        fill_path(ctx, color[3], color[3], 12 * scale);
        return can;
    };

    function create_quest_button(scale, draw_shadow, color, i) {
